<%= form_for :profile, url: profiles_path do |f| %>

  <div>
  	<div class="labelleft">
  	<%= f.label :name %>
    </div>
    <div class="textfieldright">
  	<%= f.text_field :name, :class => "textfield" %>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :location %>
    </div>
    <div class="textfieldright">
  	<%= f.text_field :location, :class => "textfield"%>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :impactfocus %>
    </div>
    <div class="textfieldright">
  	<%= f.select :impactfocus, options_for_select(['Children','Homeless','Environment','Nature','Sozial Network','Online-Communitiy']),{},:class => 'dropdownm'%>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :projecttype, 'Project Type' %>
    </div>
    <div class="textfieldright">
  	<%= f.select :projecttype, options_for_select(['Continuous','One-Time']),{},:class => 'dropdownm'%>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :projectname, 'Project Name' %>
    </div>
    <div class="textfieldright">
  	<%= f.text_field :projectname, :class => "textfield"%>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :milestones %>
    </div>
    <div class="textfieldright">
  	<%= f.select :milestones, options_for_select(['100 served/month','150 served/month','200 served/month']),{},:class => 'dropdownm'%>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :tfrom, 'from' %>
    </div>
    <div class="textfieldright">
  	<%= f.text_field :tfrom, :class => "textfield"%>
    </div>
  </div>
  
  <div>
  	<div class="labelleft">
  	<%= f.label :tto, 'to' %>
    </div>
    <div class="textfieldright">
  	<%= f.text_field :tto, :class => "textfield"%>
    </div>
  </div>
  
   <div>
  	<div class="labelleft">
  	<%= f.label :resources %>
    </div>
    <div class="textfieldright">
  	<%= f.select :resources, options_for_select(['Volunteers','Money','Equipment']),{},:class => 'dropdownm'%>
    </div>
  </div>


  <div class="buttonright">
    <%= f.submit 'next Step',:class => "submit_button"%>
  </div>
<% end %>