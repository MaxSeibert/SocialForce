<h1 class="headingBig">All Matches</h1><br />

<table class="center">
  <thead>
    <tr>
      <th>Organisation name </th>
      <th>Score </th>
      <th>Amount</th>
      <th>Staff</th>
      <th>equipment</th>
      <th>Profile_id:</th>
      

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @match.each do |match| %>
      <tr>
        <td><%= match.name %></td>
        <td><%= match.score %></td>
        <td><%= match.amount %></td>
        <td><%= match.staff %></td>
        <td><%= match.equipment %></td>
        <td><%= match.profileid %></td>
        <td><%= link_to 'Show', match, :style =>"color: red;" %></td>
        <td><%= link_to 'Destroy', match, method: :delete, data: { confirm: 'Are you sure?' }, :style =>"color: red;" %></td>
     
     </tr>
    <% end %>
  </tbody>
</table>

<br>